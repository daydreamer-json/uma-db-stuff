  "type": "module",
  "module": "dist/main.js",
  "private": true,
  "scripts": {
    "format": "npx biome format --write src",
    "start": "npx biome format --write src && tsc && node dist/main.js",
    "compile": "npx biome format --write src && tsc",
    "build": "npx biome format --write src && tsc && node dist/prebuild.js && npx nexe dist/main.js -o uma-db-stuff.exe --build"
  },
